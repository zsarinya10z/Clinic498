<!DOCTYPE html>
<html lang="en">
<%- include('../includes/head.ejs') %>

  <body id="default_theme" class="it_service blog_detail">
    <!-- loader -->
    <!-- <div class="bg_load">
      <img
        class="loader_animation"
        src="/images/loaders/leaf.png"
        alt="#"
      />
    </div> -->
    <!-- end loader -->
    <!-- header -->
    <%- include('../includes/header.ejs') %>
      <!-- end header -->
      <!-- inner page banner -->
      <%- include('../includes/inner_banner.ejs') %>
        <!-- end inner page banner -->
        <!-- section -->
        <div class="section padding_layout_1 blog_grid">
          <div class="container">
            <div class="row">
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 pull-right">
                <div class="full">
                  <div class="blog_section margin_bottom_0">
                    <div class="blog_feature_cantant">
                      <div class="row">
                        <div class="full">
                          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 contant_form">
                            <div class="form_section">
                              <form class="form_contant" method="POST" action="/create" enctype="multipart/form-data">
                                <fieldset>
                                  <div class="row">
                                    <div class="col-md-12">
                                      <div class="row">
                                        <div class="field col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                          <h4>Service</h4>
                                          <input type="hidden" name="product_id" value="" />
                                          <input class="field_custom" name="product_name" placeholder="Service" type="text"
                                            value="" required />
                                        </div>
                                      </div>

                                      <div class="row">
                                        <div class="field col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                          <h4>Categories</h4>
                                          <select class="field_custom" name="categories" id="categories" required>
                                            <option hidden>Choose</option>
                                            <option value="Dermatology">Dermatology</option>
                                            <option value="Plastic Surgery">Plastic Surgery</option>

                                          </select>
                                        </div>
                                      </div>
                                      <div class="row">
                                        <div class="field col-lg-12 col-md-6 col-sm-12 col-xs-12">
                                          <div class="col-md-12">
                                            <div class="row">
                                              <div class="field col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <h4>Price</h4>
                                                <input type="hidden" name="product_id" value="" />
                                                <input class="field_custom" name="product_name" placeholder="Price" type="text"
                                                  value="" required />
                                              </div>
                                            </div>


                                        </div>
                                      </div>
                                    </div>
                                    <div class="col-md-6">
                                      <div class="field col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                      </div>
                                    </div>
                                  </div>
                                  <div class="row">
                                    <div class="field col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                      <h4>Description</h4>
                                      <textarea class="field_custom" placeholder="Description"
                                        name="description"></textarea>
                                    </div>
                                    <div class="center">
                                      <button type="submit" class="btn main_bt">
                                        Save
                                      </button>
                                    </div>
                                  </div>
                                </fieldset>
                              </form>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- end section -->
        <!-- End Model search bar -->
        <!-- js section -->
        <%- include('../includes/low_script.ejs') %>
  </body>

</html>